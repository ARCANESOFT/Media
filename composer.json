{
    "name":        "arcanesoft/media",
    "description": "Media package for ARCANESOFT/Foundation.",
    "homepage":    "https://github.com/ARCANESOFT/Media",
    "keywords":    [
        "arcanesoft", "arcanedev", "media", "images", "files", "manager"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":             ">=7.0",
        "arcanesoft/auth": "~2.2",
        "arcanesoft/core": "~2.4"
    },
    "require-dev": {
        "arcanesoft/foundation": "~2.2",
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench-browser-kit=~3.4.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

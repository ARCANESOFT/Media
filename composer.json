{
    "name":        "arcanesoft/media",
    "description": "Media module for ARCANESOFT.",
    "keywords":    ["arcanedev", "arcanesoft", "media", "media-manager", "media-viewer", "images", "files", "framework", "laravel"],
    "license":     "MIT",
    "type":        "project",
    "authors":     [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "arcanesoft/foundation": "^4.0"
    },
    "require-dev": {
        "laravel/framework": "^8.40",
        "mockery/mockery": "^1.4.2",
        "orchestra/testbench-core": "^6.4",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Arcanesoft\\Media\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanesoft\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:dox": "phpunit --testdox --colors=always",
        "test:cov": "phpunit --coverage-html coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Arcanesoft\\Media\\MediaServiceProvider"
            ]
        },
        "arcanesoft": {
            "install": "Arcanesoft\\Media\\Console\\InstallCommand",
            "publish": "Arcanesoft\\Media\\Console\\PublishCommand"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
